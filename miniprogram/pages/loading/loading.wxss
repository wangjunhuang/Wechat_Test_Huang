  height: 100%;
  background: #f6f6f6;
 text-align: center;
}
.contianer{
  width: 100%;
  height: 100%;
  align-items: center;
  display: flex;
}
.login{
  width: 50%;
  height: auto;
   border: none;
   border-radius: 15rpx;
  background-color:#6adbdf ;
  font-size: 20px;
  color:white;
}
.login:active{
     border-radius: 15rpx;
  border: 1px #6adbdf solid;
  width: 50%;
  height: auto;
  background-color:#f6f6f6 ;
  font-size: 20px;
  color:#6adbdf;
}